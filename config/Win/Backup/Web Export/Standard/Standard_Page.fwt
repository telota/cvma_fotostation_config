<HTML>
<HEAD>
<%BACKGROUND%>
<BODY BACKGROUND="<%=FWT_BACKGROUNDFILE%>">
<%/BACKGROUND%>
<TITLE><%=FWT_TITLE%></TITLE>

<%METAKEYWORDS%>
<META NAME="KEYWORDS" CONTENT="<%=FWT_METAKEYWORDS%>"> 
<%/METAKEYWORDS%>
<link href="Extra/Standard.css" rel="stylesheet" type="text/css" />
</HEAD>
<BODY  BGCOLOR="<%=FWT_BACKGROUNDCOLOR%>" style="color:#<%=FWT_TEXTCOLOR%>">
<CENTER>
<BR>

<%LOGO%>
<IMG SRC="<%=FWT_LOGOFILE%>">
<%/LOGO%>
<br>
<H1><%=FWT_TITLE%></H1>
<%DESCRIPTION%>
<BR><H4><%=FWT_DESCRIPTION%></H4>
<%/DESCRIPTION%>
<%NAVIGATE%>
<TABLE CELLSPACING=30 CELLPADDING=0 BORDER=0>
	<TR>
		<TD ALIGN=LEFT WIDTH=150>
			<%PREVIOUS_PAGE%>
			<A HREF="<%=FWT_PREVIOUSPAGE%>" >  <%=FWT_PREVIOUSPAGEWORD%>  </A>
			<%/PREVIOUS_PAGE%>
		</TD>
		<TD ALIGN=CENTER>
			<%=FWT_DIRECTPAGE%>
		</TD>
		<TD ALIGN=RIGHT WIDTH=150>
			<%NEXT_PAGE%>
			<A HREF="<%=FWT_NEXTPAGE%>" >  <%=FWT_NEXTPAGEWORD%>  </A>
			<%/NEXT_PAGE%>
		</TD>
	</TR>
</TABLE>
<%/NAVIGATE%>

<TABLE BORDER=1 cellpadding=10 cellspacing=0 RULES=All>
	<%IMAGEROW%>
	<TR>
		<%IMAGE_WITH_PV%>
		<TD WIDTH=200 HEIGHT=200 ALIGN=CENTER VALIGN=TOP> 
		<A HREF="<%=FWT_PREVIEWPAGE%>"><IMG SRC="<%=FWT_THUMBNAIL%>" Alt="<%=FWT_IPTC120%>" border=0></A>
			<BR><%=FWT_IMAGETITLEORNAME%>
			<%DOWNLOAD%>
			<BR> <A HREF="<%=FWT_DOWNLOADIMAGE%>"><%=FWT_DOWNLOADWORD%></A>
			<%/DOWNLOAD%>
		</TD>
		<%/IMAGE_WITH_PV%>

		<%IMAGE_NO_PV%>
		<TD ALIGN=CENTER VALIGN=TOP> <IMG SRC="<%=FWT_THUMBNAIL%>" Alt="<%=FWT_IPTC120%>">
			<BR><%=FWT_IMAGETITLEORNAME%>
			<%DOWNLOAD%>
			<BR><A HREF="<%=FWT_DOWNLOADIMAGE%>"><%=FWT_DOWNLOADWORD%></A>
			<%/DOWNLOAD%>
		</TD>
		<%/IMAGE_NO_PV%>
	</TR>
	<%/IMAGEROW%>
</TABLE>
<%EMAIL%>
<A HREF="mailto:<%=FWT_EMAILADDRESS%>"><%=FWT_EMAILNAME%></A>.
<%/EMAIL%>
</CENTER>
</BODY>
</HTML>
